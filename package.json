{
  "name": "cupandpen",
  "version": "1.0.0",
  "description": "Repository for https://cupandpen.com",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "sync": "bash scripts/sync.sh",
    "sitemap": "node scripts/generateSitemap.js",
    "build": "npm run build:index && npm run build:views",
    "build:index": "node scripts/build-content.js",
    "build:views": "node scripts/build-views.js",
    "dev": "http-server ./public"
  },
  "engines": {
    "node": "^12",
    "npm": "^6"
  },
  "author": "Richie Casto (https://github.com/rcasto)",
  "license": "MIT",
  "devDependencies": {
    "ejs": "3.1.5",
    "gray-matter": "4.0.2",
    "http-server": "0.12.3",
    "jsdom": "16.4.0",
    "katex": "0.12.0",
    "markdown-it": "12.0.3",
    "markdown-it-link-attributes": "3.0.0",
    "markdown-it-texmath": "0.8.0",
    "sitemap": "6.3.3",
    "xml-formatter": "2.4.0"
  }
}
